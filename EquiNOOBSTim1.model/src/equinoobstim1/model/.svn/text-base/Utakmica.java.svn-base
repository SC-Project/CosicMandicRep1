package equinoobstim1.model;

import java.sql.Date;


public class Utakmica {
	
	private String nazivGosti;
	private String nazivDomaci;
	private int goloviGosti;
	private int goloviDomaci;
	private int brojZutihKartona;
	private int brojCrvenihKartona;
	private String stadion;
	private Date datum;
	
	public Utakmica(String nazivGosti, String nazivDomaci, int goloviGosti,
			int goloviDomaci, int brojZutihKartona, int brojCrvenihKartona,
			String stadion, Date datum) {
		super();
		this.nazivGosti = nazivGosti;
		this.nazivDomaci = nazivDomaci;
		this.goloviGosti = goloviGosti;
		this.goloviDomaci = goloviDomaci;
		this.brojZutihKartona = brojZutihKartona;
		this.brojCrvenihKartona = brojCrvenihKartona;
		this.stadion = stadion;
		this.datum = datum;
	}
	public Utakmica() {
		super();
	}
	public int getGoloviGosti() {
		return goloviGosti;
	}
	public void setGoloviGosti(int goloviGosti) {
		this.goloviGosti = goloviGosti;
	}
	public int getGoloviDomaci() {
		return goloviDomaci;
	}
	public void setGoloviDomaci(int goloviDomaci) {
		this.goloviDomaci = goloviDomaci;
	}
	public int getBrojZutihKartona() {
		return brojZutihKartona;
	}
	public void setBrojZutihKartona(int brojZutihKartona) {
		this.brojZutihKartona = brojZutihKartona;
	}
	public int getBrojCrvenihKartona() {
		return brojCrvenihKartona;
	}
	public void setBrojCrvenihKartona(int brojCrvenihKartona) {
		this.brojCrvenihKartona = brojCrvenihKartona;
	}
	public String getStadion() {
		return stadion;
	}
	public void setStadion(String stadion) {
		this.stadion = stadion;
	}
	public Date getDatum() {
		return datum;
	}
	public void setDatum(Date datum) {
		this.datum = datum;
	}
	public String getNazivGosti() {
		return nazivGosti;
	}
	public void setNazivGosti(String nazivGosti) {
		this.nazivGosti = nazivGosti;
	}
	public String getNazivDomaci() {
		return nazivDomaci;
	}
	public void setNazivDomaci(String nazivDomaci) {
		this.nazivDomaci = nazivDomaci;
	}
	public String toString() {
		return nazivDomaci + " " + goloviDomaci + ":" + goloviGosti + " " + nazivGosti + 
				"\nDatum: " + datum.toString() + "\nZutih kartoni: " + brojZutihKartona +
				"\nCrvenih kartona: " + brojCrvenihKartona + "\nStadion: " + stadion;
 	}

}
